<div class="dashboard-page">

<div class="container">

  <ul class="nav nav-tabs">
    <li><a href="#">Enrollees</a></li>
    <li><a href="#">Waiting Lists</a></li>
    <li><a href="#">Search</a></li>
  </ul>
  <div class="row">
    <div class="col-md-12">
      <a href="children/new" type="button" class="btn btn-default">Add Child +</a>
    </div>
  </div>
  <table class="table table-hover">
    <thead>
      <tr>
        <th>Name</th>
        <th>Age</th>
      </tr>
    </thead>

    {{#each kids}}
    <tr>
      <td>{{child_first_name}}
          {{child_last_name}}</td>
      <td>{{age}}</td>
    </tr>
    {{/each}}

  </table>

</div>

</div>
